   3.770678197675167*^9}, {3.7706782410549927`*^9, 3.770678291549712*^9}, {
   3.770678413791648*^9, 3.770678455663818*^9}, {3.770678534751796*^9, 
   3.770678590074169*^9}, {3.77067863479613*^9, 3.770678824585465*^9}, {
   3.770678861141759*^9, 3.7706788782218523`*^9}, {3.770679110003005*^9, 
